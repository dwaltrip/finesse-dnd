{
  "name": "finesse-dnd",
  "version": "0.1.5",
  "description": "Powerful, lower-level drag & drop library that doesn't directly touch the DOM",
  "main": "finesse-dnd.js",
  "scripts": {
    "prebuild:lib": "shx mkdir -p dist",
    "build:lib": "babel src -d dist",
    "prebuild:demo": "shx mkdir -p demo/dist",
    "build:demo": "browserify demo/app.js -t babelify --outfile demo/dist/app.js",
    "prewatch:demo": "npm run build:demo",
    "watch:demo": "watchify demo/app.js -t babelify --outfile demo/dist/app.js -v",
    "prepublish": "npm run build:lib"
  },
  "keywords": [
    "drag and drop",
    "dnd",
    "drag",
    "vdom",
    "spa",
    "finesse"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dwaltrip/finesse-dnd.git"
  },
  "author": "Daniel Waltrip",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.1",
    "mithril": "^0.2.5",
    "shx": "^0.1.4",
    "watchify": "^3.8.0"
  }
}
